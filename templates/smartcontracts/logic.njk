'use strict';

// SDK Library to asset with writing the logic
const SmartContract = require('fabric-shim').SmartContract;

/**
 * Support the {{ core.description }}
 */
class {{ core.name | replace("-","_")}} extends SmartContract {


{% for id,txs in types.transaction %}    
	constructor() {
		super('{{id}}');
	}
{% for tx in txs %}
    /** {% for id,info in tx.properties %} 
     * {{id}} {{ info.type  }}  {{info.description }} {% endfor %}
    */
	async {{tx.name}}(api,{% for id,info in tx.properties %}{{id}}{% if not loop.last %},{% endif %}{% endfor %}) {
	}
{% endfor %} 
{% endfor %}
};

module.exports =  {{ core.name | replace("-","_") }};